{
  "name": "aincrad-headers",
  "displayName": "Aincrad Headers",
  "description": "Extention for generate the Aincrad headers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aincrad-headers.generateHeader"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "aincrad-headers.generateHeader",
      "title": "Aincrad: Insert header"
    }],
    "keybindings": [
      {
        "command": "aincrad-headers.generateHeader",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
